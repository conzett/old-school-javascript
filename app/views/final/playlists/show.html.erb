<div class="container">
  <div class="row">
    <div class="col-3">
      Final form with Combobox
    </div>
    <div class="col-9">
      <table class="table table-hover table-striped table-sm">
        <tbody
          id="playlist_items"
          data-reorderable="<%= final_playlist_playlist_items_path @playlist %>">
          <%= render @playlist.playlist_items.order :position  %>
        </tbody>
      </table>
    </div>
  </div>
  <%= render partial: 'final/playlist_items/form', locals: {
    playlist_item: [:final, @playlist, PlaylistItem.new] } %>
</div>
